#include <iostream>
using namespace std;

int main()
{
    int n,temp,rem,sum=0;
    cin>>n;
    temp=n;
    while(n>0)
    {
        rem=n%10;
        sum=sum+(rem*rem*rem);
        n/=10;
        
    }
    if(temp==sum)
    cout<<"armstrong number";
    else
    cout<<"not armstrong";

    return 0;
}
